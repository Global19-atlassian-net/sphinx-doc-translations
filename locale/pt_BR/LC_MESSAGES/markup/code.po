msgid "``none`` (no highlighting)"
msgstr ""

# bfc8bac0f11445c4ac98c1ee8d12233e
#: ../../markup/code.rst:58
msgid "``python`` (the default when :confval:`highlight_language` isn't set)"
msgstr ""

# e55df2abd30a44b28f9888eb1099c6f9
#: ../../markup/code.rst:59
msgid ""
"``guess`` (let Pygments guess the lexer based on contents, only works with "
"certain well-recognizable languages)"
msgstr ""

# 70e32bd718f148eea2ea31dbc6f30bd4
#: ../../markup/code.rst:61
msgid "``rest``"
msgstr ""

# 81157ff0293e412c969574de93fba85c
#: ../../markup/code.rst:62
msgid "``c``"
msgstr ""

# c8c4beb138724dbfb0efcd2c16761737
