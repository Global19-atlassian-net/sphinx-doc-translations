msgstr "HTML出力のEpub電子書籍バージョンを生成します。"

# e071ffb78a0242f592a6aae9245d477d
#: ../../man/sphinx-build.rst:65
msgid "text"
msgstr "text"

# d54fa993d72d4826a1fb45409129c385
#: ../../man/sphinx-build.rst:65
msgid "Generates a plain-text version of the documentation."
msgstr "プレーンテキストファイルをビルドします。"

# dbac48fbef2a4051b2de1d73248d9334
#: ../../man/sphinx-build.rst:68
msgid "gettext"
msgstr "gettext"

# 3a0315026df540c38c7062ec4837fb51
#: ../../man/sphinx-build.rst:68
